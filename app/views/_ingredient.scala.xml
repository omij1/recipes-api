@(ingredient: Ingredient)
			<ingredient>
				<id>@ingredient.getId()</id>
				<name>@ingredient.getIngredientName()</name>
				<units>@ingredient.getUnits()</units>
				<version>@ingredient.getVersion()</version>
				<created>@ingredient.getCreated()</created>
				<updated>@ingredient.getUpdated()</updated>
			</ingredient>